{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Executar AutoMonitor",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/AutoMonitor",  // caminho para o execut√°vel gerado
      "args": [],                                        // argumentos para o programa, se houver
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
